<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="200" prefWidth="400" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.usercrud">
    <children>
        <Label layoutX="30" layoutY="40" text="Enter Your Name:" />
        <TextField fx:id="nameField" layoutX="150" layoutY="40" promptText="Enter your name" />
        <Button layoutX="180" layoutY="80" mnemonicParsing="false" onAction="#handlePlayButtonAction" text="Start Game" />
    </children>
</VBox>